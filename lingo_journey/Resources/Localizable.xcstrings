    "privacy.introduction.content" : {
      "localizations" : {
        "de" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Lingo Journey verpflichtet sich zum Schutz Ihrer Privatsphäre. Diese App wurde mit einem Offline-First-Ansatz entwickelt, was bedeutet, dass Ihre Daten auf Ihrem Gerät bleiben und niemals an unsere Server übertragen werden."
          }
        },
        "en" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Lingo Journey is committed to protecting your privacy. This app is designed with an offline-first approach, meaning your data stays on your device and is never transmitted to our servers."
          }
        },
        "es" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Lingo Journey se compromete a proteger su privacidad. Esta aplicación está diseñada con un enfoque offline-first, lo que significa que sus datos permanecen en su dispositivo y nunca se transmiten a nuestros servidores."
          }
        },
        "fr" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Lingo Journey s'engage à protéger votre vie privée. Cette application est conçue avec une approche hors ligne d'abord, ce qui signifie que vos données restent sur votre appareil et ne sont jamais transmises à nos serveurs."
          }
        },
        "ja" : {
          "stringUnit" : {
            "state" : "translated",
